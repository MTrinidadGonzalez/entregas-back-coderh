    {{/if}}



<div>
{{#if productsJSON}}
 <h1>Productos de:{{userAlias}} </h1>
  <div class="div-premium-products">  
    {{#each productsJSON}}
      <div class="productCard">
        <img src={{this.img}} alt={{this.description}}>
        <p>Description: {{this.description}} </p>
        <p>Price: $ {{this.price}} </p>
        {{#if ../isAdmin}}
          <button class="btnDeleteUserProduct" data-product-id="{{this._id}}">Delete Product</button>
        {{/if}}

        {{#if ../isUserOrPremium}}
          <button id="btn-subtract-product" class="btn-subtract-product">-</button>
          <span id="spam-quantity" class="spam-quantity">1</span>
          <button id="btn-add-more-product" class="btn-add-more-product"  data-stock="{{this.stock}}">+</button>
          <button class="btnCard"  data-product-id="{{this._id}}" >Add to cart</button>
        {{/if}}
      </div>

    {{/each}}
  </div>
{{else}}
  <h1>No hay productos de este usuario por el momento</h1>
{{/if}}
</div>




 <script src="/js/products.js"></script> 