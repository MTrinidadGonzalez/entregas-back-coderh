        <li>
         <a href="/cart" class="nav-item" >        
           <span class="material-symbols-outlined">
            shopping_cart
            </span>  
            <samp>{{totalQuantity}}</samp>
         </a>
        </li>
       <li><a href="/users/users" class="nav-item" >Ver Usuarios</a></li>
        {{#if isUser}}
           <li><a href="/profileuser" style="color: white;"  class="nav-item">Mi perfil
           <span>  <img src={{imgProfile}} alt="Imagen de perfil de usuario" class="imgProfileSpam"></span>
           </a></li>
       
